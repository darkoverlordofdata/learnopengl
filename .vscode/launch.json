{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/",
            "sourceMaps": true,
            "webRoot": ".",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ]
        },
    
        {
            "name": "Attach",
            "type": "chrome",
            "request": "attach",
            "port": 9222
        }
    ]
}